<p id="notice"><%= notice %></p>

<h1>Listing Order Audits</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Datetime</th>
      <th>Status from</th>
      <th>Status to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_audits.each do |order_audit| %>
      <tr>
        <td><%= order_audit.order %></td>
        <td><%= order_audit.datetime %></td>
        <td><%= order_audit.status_from %></td>
        <td><%= order_audit.status_to %></td>
        <td><%= link_to 'Show', order_audit %></td>
        <td><%= link_to 'Edit', edit_order_audit_path(order_audit) %></td>
        <td><%= link_to 'Destroy', order_audit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order audit', new_order_audit_path %>
